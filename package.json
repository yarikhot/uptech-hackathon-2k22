{
  "name": "you-are",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/auth": "^4.5.10",
    "@aws-amplify/core": "^4.3.11",
    "@aws-amplify/ui-react": "^1.2.26",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.5",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.1",
    "@mui/styled-engine-sc": "^5.1.0",
    "@mui/x-data-grid": "^5.12.2",
    "@mui/x-date-pickers": "^5.0.0-beta.6",
    "@reduxjs/toolkit": "^1.8.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "aws-amplify": "^4.3.11",
    "axios": "^0.24.0",
    "dayjs": "^1.11.3",
    "latest": "^0.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.isequal": "^4.5.0",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.2",
    "react-hook-form": "^7.22.4",
    "react-is": "^18.1.0",
    "react-query": "^3.39.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "6",
    "react-scripts": "^4.0.3",
    "styled-components": "^5.3.5",
    "timezones-list": "^3.0.1",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "HTTPS=true craco start",
    "build": "CI=false craco build",
    "test": "craco test",
    "eject": "craco eject",
    "prettier": "prettier --check 'src/**/*.{js,ts,tsx,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{js,ts,tsx,json}'",
    "lint": "eslint 'src/*.{js,ts,tsx}'",
    "prepare": "husky install"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "babel-loader": "8.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "airbnb"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "settings": {
      "import/parsers": {
        "@typescript-eslint/parser": [
          ".ts",
          ".tsx"
        ]
      },
      "import/resolver": {
        "typescript": {}
      }
    },
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "import/prefer-default-export": "off",
      "react/prop-types": "off",
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "react/react-in-jsx-scope": "off",
      "comma-dangle": "off",
      "no-shadow": "off",
      "object-curly-newline": "off",
      "react/jsx-one-expression-per-line": "off",
      "implicit-arrow-linebreak": "off",
      "max-len": "off",
      "operator-linebreak": "off",
      "indent": "off",
      "function-paren-newline": "off",
      "camelcase": "off",
      "react/jsx-curly-newline": "off",
      "no-confusing-arrow": "off",
      "comma-spacing": "off",
      "react/require-default-props": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "react/function-component-definition": "off",
      "@typescript-eslint/ban-types": "off",
      "react/jsx-wrap-multilines": "off",
      "import/no-cycle": "warn",
      "@typescript-eslint/no-shadow": [
        "error"
      ],
      "react/jsx-indent": "off",
      "react/jsx-filename-extension": [
        "warn",
        {
          "extensions": [
            ".tsx"
          ]
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "ts": "never",
          "tsx": "never"
        }
      ],
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "off",
      "@typescript-eslint/type-annotation-spacing": [
        "error",
        {
          "before": false,
          "after": true,
          "overrides": {
            "arrow": {
              "before": true,
              "after": true
            }
          }
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@types/jest": "^26.0.15",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.37",
    "@types/react-datepicker": "^4.4.2",
    "@types/react-dom": "^17.0.0",
    "@types/sortablejs": "^1.10.7",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "craco-alias": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.1.2",
    "prettier": "2.4.1",
    "react-error-overlay": "^6.0.9"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write",
      "yarn test --watchAll=false --findRelatedTests --bail"
    ]
  }
}
